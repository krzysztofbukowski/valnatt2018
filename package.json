{
  "version": "1.0.0",
  "author": "Krzysztof Bukowski",
  "scripts": {
    "build": "webpack",
    "pretify": "prettier --write --parser typescript 'src/**/*.{ts,tsx}'",
    "start": "webpack-dev-server",
    "test": "jest",
    "tslint": "tslint --project tsconfig.json"
  },
  "dependencies": {
    "precise-commits": "^1.0.2",
    "react": "16.4.1",
    "react-dom": "16.4.2",
    "react-redux": "5.0.7",
    "redux": "^4.0.0",
    "redux-query-sync": "0.1.7",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.11",
    "@types/enzyme-adapter-react-16": "1.0.2",
    "@types/jest": "23.1.5",
    "@types/react": "16.4.6",
    "@types/react-dom": "16.0.6",
    "@types/react-redux": "^6.0.6",
    "autoprefixer": "9.0.1",
    "babel-core": "6.26.3",
    "babel-jest": "23.4.0",
    "babel-loader": "7.1.5",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "1.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "1.1.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "^3.0.3",
    "jest": "^24.4.0",
    "jest-css-modules": "^1.1.0",
    "mini-css-extract-plugin": "0.4.1",
    "node-sass": "^4.13.1",
    "postcss": "7.0.1",
    "postcss-flexbugs-fixes": "4.0.0",
    "postcss-loader": "2.1.6",
    "prettier": "^1.18.2",
    "react-test-renderer": "16.4.1",
    "redux-logger": "^3.0.6",
    "sass-loader": "7.0.3",
    "style-loader": "0.21.0",
    "ts-jest": "^24.0.0",
    "ts-loader": "4.4.2",
    "tslint": "5.10.0",
    "typescript": "2.9.2",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "4.15.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "^3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits"
    }
  },
  "jest": {
    "setupFiles": [
      "./src/setupTests.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.s?css$": "jest-css-modules"
    },
    "testRegex": "((\\.|/)(test))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true
  },
  "proxy": "http://localhost:5000"
}
